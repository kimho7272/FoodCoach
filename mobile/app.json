{
  "expo": {
    "name": "FoodCoach powered by AI",
    "slug": "foodcoach-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "foodcoach",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#10b981"
    },
    "ios": {
      "bundleIdentifier": "com.xgoldtiger.foodcoach",
      "infoPlist": {
        "NSHealthShareUsageDescription": "Read and understand health data to provide personalized coaching.",
        "NSHealthUpdateUsageDescription": "Share workout data with other apps to keep your health data consistent.",
        "NSContactsUsageDescription": "Allow FoodCoach to access your contacts to help you find friends."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#10b981"
      },
      "package": "com.xgoldtiger.foodcoach",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow FoodCoach to access your camera to analyze your food."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysPermission": "Allow FoodCoach to use your location to find nearby healthy food options."
        }
      ],
      "expo-web-browser",
      "react-native-health-connect",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "24f55ee2-a6cf-47cc-8520-5fa137f600d5"
      }
    }
  }
}